    "express": "^4.16.3",
    "firebase": "^8.3.2",
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^5.3.16",
    "react-date-picker": "^8.1.0",
    "react-moment": "^1.1.1",
    "react-router-dom": "^5.2.0",
    "react-time-picker": "^4.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "styled-components": "^5.2.1"
  }
